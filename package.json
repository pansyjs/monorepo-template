    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "nx": "^19.4.1",
    "pnpm": "^9.5.0",
    "signale": "^1.4.0",
    "simple-git": "^3.25.0",
    "stylelint": "^16.6.1",
    "syncpack": "^12.3.3",
    "tsup": "^8.1.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "vitest": "^1.6.0",
    "walrus": "workspace:*",
    "zx": "^8.1.4"

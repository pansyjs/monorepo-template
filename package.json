    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint-react/eslint-plugin": "^1.5.15",
    "@nx/cypress": "^19.4.1",
    "@nx/react": "^19.4.1",
    "@nx/vite": "^19.4.1",
    "@nx/workspace": "^19.4.1",
    "@types/signale": "^1.4.7",
    "@vitest/ui": "^1.6.0",
    "chalk": "^5.3.0",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "dayjs": "^1.11.11",
    "eslint": "~8.57.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-react-hooks": "^4.6.2",
